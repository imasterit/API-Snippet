{
  "name": "takweya-bot",
  "version": "1.0.0",
  "description": "Students find teachers using Telegram and wizIQ",
  "main": "index.js",
  "scripts": {
    "start": "sls offline --skipCacheInvalidation -c",
    "build": "NODE_ENV=prod sls webpack --display-error-details --display-modules --compilers js:@babel/register",
    "watch": "sls webpack --watch -O .webpack/service",
    "test": "export SERVERLESS_TEST=1 && export NTBA_FIX_319=true && sls invoke test --compilers js:@babel/register",
    "test-cov": "nyc --reporter=html sls invoke test --compilers js:@babel/register --require test/env.js",
    "test-unit": "mocha --require test/env.js --require @babel/register 'test/**/**.test.js'",
    "invoke": "sls invoke local --function handle",
    "update": "aws lambda update-function-code --function-name takweya-bot --zip-file fileb://dist.zip --publish",
    "deploydev": "set NODE_ENV=dev && sls deploy --verbose --stage dev",
    "deployprod": "set NODE_ENV=prod && sls deploy --verbose --stage prod"
  },
  "repository": "",
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/register": "^7.4.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "https-proxy-agent": "^2.2.2",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.0",
    "rewire": "^4.0.1",
    "sandwich-stream": "^2.0.2",
    "serverless-mocha-plugin": "^1.10.0",
    "serverless-offline": "^5.6.0",
    "serverless-webpack": "^5.3.1",
    "sinon": "^7.3.2",
    "socks-proxy-agent": "^4.0.2",
    "socks5-https-client": "^1.2.1",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.35.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.1",
    "@hapi/joi": "^15.1.0",
    "aws-sdk": "^2.521.0",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "copy-webpack-plugin": "^5.0.4",
    "debug": "^4.1.1",
    "email-templates": "^6.0.3",
    "entities": "^2.0.0",
    "formidable": "^1.2.1",
    "generate-password": "^1.4.2",
    "i18next": "^17.0.6",
    "javascript-state-machine": "^3.1.0",
    "joi-phone-number": "^3.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.2.7",
    "mongoose": "^5.6.3",
    "node-telegram-bot-api": "^0.30.0",
    "nodemailer": "^6.3.0",
    "querystring": "^0.2.0",
    "request": "^2.88.0",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "xml2js": "^0.4.19"
  },
  "proxy": "http://127.0.0.1:1087"
}
